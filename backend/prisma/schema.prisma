

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
 model User {
  id Int @id @default(autoincrement())
  name String?
  username String @unique
  password String
  blog Blog[]
 }


model Blog {
  id Int @id @default(autoincrement())
  authorID Int 
  content String
  published Boolean @default(false)
  title String
  author User @relation(fields: [authorID],references: [id])
}